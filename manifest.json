{
    "name": "UVic Courses to Calendar",
    "version": "1.0",
    "description": "Convert your courses to calendar events with the click of a button!",
    "permissions": ["activeTab", "downloads", "notifications", "https://www.uvic.ca/mypage/f/my-home/p/mycourses*", "declarativeContent", "storage"],
    "background": {
        "scripts":["ics.js-master/ics.deps.min.js", "background.js"],
        "persistent": false
    },

    "browser_action": {},
    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    
    "manifest_version": 2
}